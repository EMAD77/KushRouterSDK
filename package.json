{
  "name": "kushrouter-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for KushRouter - Unified LLM API with 200+ AI models",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "chatbot",
    "sdk",
    "typescript",
    "kushrouter"
  ],
  "author": "KushRouter Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EMAD77/KushRouterSDK.git"
  },
  "homepage": "https://kushrouter.com",
  "bugs": {
    "url": "https://github.com/EMAD77/KushRouterSDK/issues"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}